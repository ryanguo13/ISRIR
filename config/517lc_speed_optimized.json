{
  "name": "sr_ffhq_train_speed_optimized",
  "phase": "train",
  "gpu_ids": [
    0
  ],
  "log_wandb_ckpt": true,
  "log_eval": false,
  "enable_wandb": true,
  "path": {
    "log": "F:/SR3_training_result/logs",
    "tb_logger": "F:/SR3_training_result/tb_logger",
    "results": "F:/SR3_training_result/results",
    "checkpoint": "F:/SR3_training_result/checkpoint",
    "resume_state": "dataset/I640000_E37",
    "param_output": "F:/SR3_training_result/param_outputs"
  },
  "datasets": {
    "train": {
      "name": "FFHQ",
      "mode": "LRHR",
      "dataroot": "dataset/ffhq_16_128_16_128",
      "datatype": "lmdb",
      "l_resolution": 16,
      "r_resolution": 128,
      "batch_size": 8,
      "num_workers": 16,
      "use_shuffle": true,
      "data_len": -1,
      "pin_memory": true,
      "drop_last": true
    },
    "val": {
      "name": "FFHQ",
      "mode": "LRHR",
      "dataroot": "dataset/ffhq_raw_images_prepared_16_128",
      "datatype": "img",
      "l_resolution": 16,
      "r_resolution": 128,
      "data_len": 2
    }
  },
  "model": {
    "which_model_G": "sr3",
    "finetune_norm": false,
    "unet": {
      "in_channel": 6,
      "out_channel": 3,
      "inner_channel": 64,
      "channel_multiplier": [
        1,
        2,
        4,
        8,
        8
      ],
      "attn_res": [
        16
      ],
      "res_blocks": 2,
      "dropout": 0.1
    },
    "beta_schedule": {
      "train": {
        "schedule": "linear",
        "n_timestep": 2000,
        "linear_start": 1e-06,
        "linear_end": 0.01
      },
      "val": {
        "schedule": "linear",
        "n_timestep": 50,
        "linear_start": 1e-06,
        "linear_end": 0.01
      }
    },
    "diffusion": {
      "image_size": 128,
      "channels": 3,
      "conditional": true
    }
  },
  "train": {
    "n_iter": 650000,
    "val_freq": 5000,
    "save_checkpoint_freq": 5000,
    "print_freq": 500,
    "save_param_freq": 50000,
    "optimizer": {
      "type": "adamw",
      "lr": 0.0002,
      "weight_decay": 0.01
    },
    "ema_scheduler": {
      "step_start_ema": 2000,
      "update_ema_every": 1,
      "ema_decay": 0.9999
    }
  },
  "wandb": {
    "project": "sr_ffhq_train_speed_optimized"
  },
  "param_logging": {
    "enabled": false,
    "save_gradients": false,
    "save_weights": false,
    "save_activations": false,
    "save_noise_schedule": false,
    "save_loss_components": false,
    "layers_to_monitor": []
  }
} 